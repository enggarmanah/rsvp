<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>

		<div id="profile-wrapper">
			<div id="profile-content">
				
				<div id="social-info">
					<div id="social-info-title">
						<g:Label styleName="content" text="Komentar Anda"/>
					</div>
					<div id="social-comments">
						<div class="comment-add">
							<div class="fb-image">
								<g:Image ui:field="fbProfileImg"></g:Image>
							</div>
							<div class="comment-details">
								<g:TextArea ui:field="textTa" styleName="text-input"></g:TextArea>
							</div>
						</div>
				</div>
					<div id="promo-booking">
						<g:Button ui:field="okBtn" text="OK" styleName="button" width="75px"/>
						<g:Button ui:field="cancelBtn" text="Batal" styleName="button" width="75px"/>
					</div>
				</div>
				
			</div>
		</div>

	</g:HTMLPanel>

</ui:UiBinder> 