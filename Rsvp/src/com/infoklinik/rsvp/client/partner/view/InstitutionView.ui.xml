<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>
		
		<div id="partner">
			<div id="partner-logo">
				<g:SimplePanel ui:field="logoPanel"></g:SimplePanel>
			</div>
			<div id="partner-info">
				<div class="partner-name">
					<g:Label ui:field="nameLb"></g:Label>
				</div>
				<div class="partner-desc">
					<g:Label ui:field="profileLb"></g:Label>
				</div>
				<div class="partner-contact">
					<div class="icon">
						<img src="images/building.png" class="contact-img" />
					</div>
					<div class="info">
						<g:Label ui:field="addressLb"></g:Label>
					</div>
					<div class="icon">
						<img src="images/phone.png" class="contact-img" />
					</div>
					<div class="info">
						<g:Label ui:field="telephoneLb"></g:Label>
					</div>
					<div class="icon">
						<img src="images/mail.png" class="contact-img" />
					</div>
					<div class="info">
						<g:Label ui:field="emailLb"></g:Label>
					</div>
				</div>
			</div>
		</div>
		
	</g:HTMLPanel>

</ui:UiBinder> 