<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel ui:field="contentPanel">

		<div id="popup-edit-wrapper">
			<div id="popup-edit">
				<div class="title">Hari :</div>
				<div class="info">
				    <g:ListBox ui:field="dayLb" styleName="select"></g:ListBox>
				</div>
				<div class="title">Jam Mulai :</div>
				<div class="info">
				    <g:ListBox ui:field="opHourStartLb" styleName="select"></g:ListBox>
				</div>
				<div class="title">Jam Berakhir :</div>
				<div class="info">
				    <g:ListBox ui:field="opHourEndLb" styleName="select"></g:ListBox>
				</div>
				<div class="title">
				</div>
				<div class="info">
					<g:Button ui:field="okBtn" text="OK" styleName="button"/>
					<g:Button ui:field="cancelBtn" text="Batal" styleName="button"/>
				</div>
			</div>
		</div>

	</g:HTMLPanel>

</ui:UiBinder> 