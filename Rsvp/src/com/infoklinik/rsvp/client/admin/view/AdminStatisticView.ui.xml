<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel ui:field="contentPanel">

		<div id="statistic-title">
			<g:Image url="images/statistic.png" styleName="image"/>
		</div>
		<div id="statistic-info">
			Statistik penggunaan layanan portal informasi kesehatan.
		</div>
		<div id="statistic">
			<div class="title">
				<img src="images/search.png" class="admin-stat-img" />
				Total Pencarian :
			</div>
			<div class="info">
				<g:Label ui:field="searchTotalLb"/>
			</div>
			<div class="sub-statistic-wrapper">
				<div class="sub-statistic">
					<div class="sub-title">Klinik :</div>
					<div class="sub-info">
						<g:Label ui:field="searchClinicLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Rumah Sakit :</div>
					<div class="sub-info">
						<g:Label ui:field="searchHospitalLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Laboratorium :</div>
					<div class="sub-info">
						<g:Label ui:field="searchLabLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Apotek :</div>
					<div class="sub-info">
						<g:Label ui:field="searchPharmacyLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Dokter :</div>
					<div class="sub-info">
						<g:Label ui:field="searchDoctorLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Layanan :</div>
					<div class="sub-info">
						<g:Label ui:field="searchServiceLb"/>
					</div>
				</div>
			</div>
		</div>
		<div id="statistic">
			<div class="title">
				<img src="images/case.png" class="admin-stat-img" />
				Total Reservasi :
			</div>
			<div class="info">
				<g:Label ui:field="apptTotalLb"/>
			</div>
			<g:FlowPanel ui:field="apptPanel" styleName="sub-statistic-wrapper"></g:FlowPanel>
		</div>
		<div id="statistic">
			<div class="title">
				<img src="images/calculator.png" class="admin-stat-img" />
				Total Data :
			</div>
			<div class="info">
				<g:Label ui:field="dataTotalLb"/>
			</div>
			<div class="sub-statistic-wrapper">
				<div class="sub-statistic">
					<div class="sub-title">Klinik :</div>
					<div class="sub-info">
						<g:Label ui:field="dataClinicLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Rumah Sakit :</div>
					<div class="sub-info">
						<g:Label ui:field="dataHospitalLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Laboratorium :</div>
					<div class="sub-info">
						<g:Label ui:field="dataLabLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Apotek :</div>
					<div class="sub-info">
						<g:Label ui:field="dataPharmacyLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Dokter :</div>
					<div class="sub-info">
						<g:Label ui:field="dataDoctorLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Layanan :</div>
					<div class="sub-info">
						<g:Label ui:field="dataServiceLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Asuransi :</div>
					<div class="sub-info">
						<g:Label ui:field="dataInsuranceLb"/>
					</div>
				</div>
			</div>
		</div>
		<div id="statistic">
			<div class="title">
				<img src="images/sign.png" class="admin-stat-img" />
				Metode Pencarian :
			</div>
			<div class="info">
				<g:Label ui:field="searchMethodTotalLb"/>
			</div>
			<div class="sub-statistic-wrapper">
				<div class="sub-statistic">
					<div class="sub-title">Nama :</div>
					<div class="sub-info">
						<g:Label ui:field="searchMethodNameLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Jalan :</div>
					<div class="sub-info">
						<g:Label ui:field="searchMethodStreetLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Wilayah :</div>
					<div class="sub-info">
						<g:Label ui:field="searchMethodRegionLb"/>
					</div>
				</div>
				<div class="sub-statistic">
					<div class="sub-title">Jarak :</div>
					<div class="sub-info">
						<g:Label ui:field="searchMethodDistanceLb"/>
					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 