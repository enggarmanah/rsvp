<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>
		
		<div id="popup-edit">
			<div class="title">Logo :</div>
			<div class="info">
				<g:SimplePanel ui:field="logoPanel" styleName="gallery-logo"/>
			</div>
			<div class="title"></div>
			<div class="info">
				<g:SimplePanel ui:field="uploadLogoPanel"/>
			</div>
			<div class="title">Galeri :</div>
			<div class="info">
				<g:FlowPanel ui:field="galleryPanel" styleName="gallery-gallery"/>
			</div>
			<div class="title"></div>
			<div class="info">
				<g:FocusPanel ui:field="addGalleryBtn">
				    <g:FlowPanel styleName="info-add">
				        <g:Image url="images/case.png" styleName="menu-img"></g:Image>
				        <g:Label text="Tambah Galeri" styleName="menu-label"></g:Label>
				    </g:FlowPanel>
			    </g:FocusPanel>
			</div>
		</div>
				
	</g:HTMLPanel>
</ui:UiBinder> 