<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>
		
		<div class="doctor">
				<g:FocusPanel ui:field="nameLinkLb">
					<g:HTMLPanel>
					<g:Label ui:field="nameLb"></g:Label>
					<span class="speciality">
						<g:Label ui:field="specialityLb"></g:Label>
					</span>
					</g:HTMLPanel>
				</g:FocusPanel>
			</div>
			<div class="schedule">
				<div class="icon">
					<img src="images/calendar.png" class="contact-img"/>
				</div>
				<div class="content">
					<g:FlowPanel ui:field="schedulePanel"></g:FlowPanel>
					<div class="divider"></div>
					<g:FocusPanel ui:field="addScheduleBtn">
					    <g:FlowPanel styleName="info-add">
					        <g:Image url="images/case.png" styleName="menu-img"></g:Image>
					        <g:Label text="Tambah Jadwal" styleName="menu-label"></g:Label>
					    </g:FlowPanel>
				    </g:FocusPanel>
				</div>
				<g:Image ui:field="deleteImg" url="images/delete.png"/>
			</div>
			<div class="divider"></div>
				
	</g:HTMLPanel>
</ui:UiBinder> 