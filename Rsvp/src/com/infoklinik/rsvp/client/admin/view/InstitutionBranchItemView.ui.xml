<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>
		
		<div class="profile-inst-branch">
		    <div>
		        <div class="branch-info">
					<div class="profile-inst-branch-name-edit">
						<g:Label ui:field="nameLb"></g:Label>
					</div>
					<div class="profile-inst-branch-contact-edit">
						<div class="inst-subinfo-content">
							<g:Label ui:field="addressLb" styleName="contact-info"/>
						</div>
					</div>
				</div>
				<div class="branch-delete">
				    <g:Image ui:field="deleteImg" url="images/delete.png"/>
				</div>
			</div>			
		</div>
				
	</g:HTMLPanel>
</ui:UiBinder> 