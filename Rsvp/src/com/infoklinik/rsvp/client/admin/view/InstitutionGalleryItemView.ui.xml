<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>
		
		<g:VerticalPanel styleName="gallery-item">
		    <g:FocusPanel ui:field="updateBtn">
				<g:SimplePanel ui:field="imagePanel" styleName="gallery-image"/>
			</g:FocusPanel>
			<g:FocusPanel ui:field="deleteBtn" styleName="gallery-button-wrapper">
				<g:FlowPanel styleName="gallery-button">
					<g:Image ui:field="deleteImg" url="images/delete.png" styleName="delete-icon"/>
					<g:Label text="Hapus" styleName="delete-text"></g:Label>
			    </g:FlowPanel>
		    </g:FocusPanel>
		</g:VerticalPanel>
						
	</g:HTMLPanel>
</ui:UiBinder> 