<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>

		<div id="popup-edit-wrapper">
			<g:SimplePanel ui:field="editPanel">
			    <g:HTMLPanel>
					
					<div id="popup-edit">
						<div class="map-location">
							<div id="map_canvas" style="width:100%; height:100%" class="profile-inst-map"></div>
						</div>
					</div>
							
				</g:HTMLPanel>
			</g:SimplePanel>
			<div id="popup-edit-button-right">
				<g:Button ui:field="okBtn" text="OK" styleName="button" width="75px"/>
			</div>
		</div>

	</g:HTMLPanel>

</ui:UiBinder> 