<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>

		<div id="popup-edit-wrapper">
			<g:SimplePanel ui:field="editPanel">
			    <g:HTMLPanel>
					
					<div id="popup-edit">
						<div class="paragraph">
							Silahkan input kode verifikasi yang telah dikirim ke handphone anda.
						</div>
						<div class="title">Kode Verifikasi :</div>
						<div class="info">
							<g:TextBox ui:field="verificationCodeTb" styleName="text"></g:TextBox>
						</div>
						<div class="title">Nama :</div>
						<div class="info">
							<g:TextBox ui:field="patientNameTb" styleName="text"></g:TextBox>
						</div>
						<div class="title">Jenis Kelamin :</div>
						<div class="info">
							<g:RadioButton name="sex" ui:field="patientSexMaleRb" text="Laki-laki" styleName=""></g:RadioButton>
							<g:RadioButton name="sex" ui:field="patientSexFemaleRb" text="Perempuan" styleName=""></g:RadioButton>
						</div>
						<div class="title">Tahun Lahir :</div>
						<div class="info">
							<g:TextBox ui:field="patientBirthYearTb" styleName="text"></g:TextBox>
						</div>
						<div class="title">Email :</div>
						<div class="info">
							<g:TextBox ui:field="patientEmailTb" styleName="text"></g:TextBox>
						</div>
					</div>
							
				</g:HTMLPanel>
			</g:SimplePanel>
			<div id="popup-edit-button">
				<div class="title"></div>
				<div class="info">
					<g:Button ui:field="okBtn" text="OK" styleName="button" width="75px"/>
					<g:Button ui:field="cancelBtn" text="Batal" styleName="button" width="75px"/>
				</div>
			</div>
		</div>

	</g:HTMLPanel>

</ui:UiBinder> 