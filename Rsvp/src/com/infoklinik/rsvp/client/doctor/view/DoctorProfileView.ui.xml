<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel>
		
		<div id="profile-wrapper">
			<div id="profile-menu-dokter-wrapper">
				<div id="profile-menu-dokter">
					<ul>
						<g:Label ui:field="scheduleMenuLb" styleName="menu">Jadwal Praktek</g:Label>
						<g:Label ui:field="locationMenuLb" styleName="menu">Peta Lokasi</g:Label>
						<g:Label ui:field="infoMenuLb" styleName="menu">Profil Dokter</g:Label>
					</ul>
				</div>
			</div>
			<div id="profile-content">
				<div id="profile-divider"></div>
				<div id="profile-doctor-photo">
					<g:Image ui:field="profileImg" url="images/doctor.jpg" width="115px"></g:Image>
				</div>
				<div id="profile-doctor-info">
					<g:Label ui:field="nameLb" styleName="name"></g:Label>
					<g:Label ui:field="specialityLb"></g:Label>
				</div>
				
				<g:SimplePanel ui:field="infoPanel" styleName="profile-inst-section"></g:SimplePanel>
				
				<div id="profile-inst-button">
					<g:Button ui:field="okBtn" text="OK" styleName="button" width="75px"/>
				</div>
			</div>
		</div>

	</g:HTMLPanel>
</ui:UiBinder> 