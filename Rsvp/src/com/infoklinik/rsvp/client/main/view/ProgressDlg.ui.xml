<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<g:HTMLPanel ui:field="mainPanel">
		
		<div class="form-wrap-progress-bar">
			
			<table>
				<tr>
					<td width="48px"><g:Image url="images/loading.gif" styleName="loading-img" ui:field="progressImage"></g:Image></td>
					<td class="progress-text"><g:Label ui:field="progressText"></g:Label></td>
				</tr>
			</table>
			
		</div>
		
	</g:HTMLPanel>

</ui:UiBinder> 